{
  "bitwise": true,
  "eqeqeq": true,
  "latedef": true,
  "noempty": true,
  "nonew": true,
  "trailing": true,
  "undef": true,
  "unused": true,
  "strict": false,
  "white": false,

  "node": true,
  "browser": true,

  "indent": 2,

  "maxerr": 1000,
