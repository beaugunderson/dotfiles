    "process",
    "require",

    "$",
    "_",
    "async",
    "moment",
    "google",
    "request",
    "sprintf",

    "__dirname",

    "btoa",

    "document",
    "window",

    "clearTimeout",
    "setTimeout",

    "setInterval",
    "clearInterval",

    "should",

    "describe",
    "xdescribe",
    "before",
    "beforeEach",
    "afterEach",
    "after",
